{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": ["/*"],
      "tests": ["tests/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@data/*": ["data/*"],
      "@types/*": ["types/*"],
      "@hooks/*": ["hooks/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
